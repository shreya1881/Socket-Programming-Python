    "import socket\n",
    "s=socket.socket(socket.AF_INET,socket.SOCK_DGRAM)\n",
    "host=socket.gethostname()\n",
    "port=6790\n",
    "s.bind((host,port))\n",
    "print('Server is working...')\n",
    "while True:\n",
    "    c,addr=s.recvfrom(1024)\n",
    "    print(\"Message: \",c)"
   ]
